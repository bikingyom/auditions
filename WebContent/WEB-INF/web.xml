<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>Accueil</servlet-name>
    <servlet-class>com.bikingyom.servlets.Accueil</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AuditionGestion</servlet-name>
    <servlet-class>com.bikingyom.servlets.AuditionGestion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuditionGestion</servlet-name>
    <url-pattern>/auditiongestion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MorceauGestion</servlet-name>
    <servlet-class>com.bikingyom.servlets.MorceauGestion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MorceauGestion</servlet-name>
    <url-pattern>/morceaugestion</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EleveGestion</servlet-name>
    <servlet-class>com.bikingyom.servlets.EleveGestion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EleveGestion</servlet-name>
    <url-pattern>/elevesgestion</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
</web-app>